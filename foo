import sys
import os
import subprocess

from ansi2html import Ansi2HTMLConverter
conv = Ansi2HTMLConverter()



os.system("ls -ltrs")

testMe = subprocess.check_output(["ls", "-ltrs"],  cwd="/home/vikrant")

p = subprocess.Popen('ls', shell=True, stdout=subprocess.PIPE, stderr=subprocess.STDOUT)
for line in p.stdout.readlines():
    print (line)

print (testMe)

#ansi = "".join(sys.stdin.readline())
html = conv.convert("test")
print (html)
